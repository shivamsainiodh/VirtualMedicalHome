<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">


<web-app>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login/*</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>DoctorList</servlet-name>
    <servlet-class>com.controller.DoctorList</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DoctorList</servlet-name>
    <url-pattern>/listOfDoctor/*</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
    <servlet-name>AppointmentSave</servlet-name>
    <servlet-class>com.controller.AppointmentSave</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AppointmentSave</servlet-name>
    <url-pattern>/appointmentSave/*</url-pattern>
  </servlet-mapping>
  
  
  
   <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout/*</url-pattern>
  </servlet-mapping>
  
  
    <servlet>
    <servlet-name>DoctorServlet</servlet-name>
    <servlet-class>com.controller.DoctorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DoctorServlet</servlet-name>
    <url-pattern>/doctorSignup/*</url-pattern>
  </servlet-mapping>  
  
    <servlet>
    <servlet-name>UpdatePatient</servlet-name>
    <servlet-class>com.controller.UpdatePatient</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdatePatient</servlet-name>
    <url-pattern>/updatePatient/*</url-pattern>
  </servlet-mapping>  
    
   <servlet>
    <servlet-name>PatientSignupServlet</servlet-name>
    <servlet-class>com.controller.PatientSignupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PatientSignupServlet</servlet-name>
    <url-pattern>/patientsignup/*</url-pattern>
  </servlet-mapping>
  
  
  
  
     
   <servlet>
    <servlet-name>ViewAndUpdatedoctor</servlet-name>
    <servlet-class>com.controller.ViewAndUpdatedoctor</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewAndUpdatedoctor</servlet-name>
    <url-pattern>/viewAndUpdatedoctor/*</url-pattern>
  </servlet-mapping>
  
</web-app>   